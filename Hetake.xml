<?xml version="1.0"?>
<map proto="1.3.3">
  <name>Hetake</name>
  <version>v1.0</version>
  <objective>Be the king of the mud.</objective>
  <authors>
    <author>jollee999</author>
  </authors>
  <contributors>
    <contributor contribution="XML">Guicotsch</contributor>
    <contributor contribution="Minor buildings">DanikasTheMan</contributor>
    <contributor contribution="Buildings, Feedbacks">entergame1994</contributor>
    <contributor contribution="Flag Specialist">DoomRobotBoy</contributor>
  </contributors>
  <teams>
    <team color="purple" max="20" max-overfill="23">Purple Team</team>
    <team color="aqua" max="20" max-overfill="23">Cyan Team</team>
  </teams>
  <rules>
    <rule>Be the first team to earn 400 points.</rule>
  </rules>
  <kits>
    <kit name="spawn" force="true">
      <item slot="0">iron sword</item>
      <item slot="1">bow</item>
      <item slot="2">cooked salmon</item>
      <item slot="8">golden apple</item>
      <item slot="28" amount="16">arrow</item>
    </kit>
    <kit name="purple" parents="spawn">
      <helmet color="7F3FB2">leather helmet</helmet>
      <chestplate>gold chestplate</chestplate>
      <leggings>chainmail leggings</leggings>
      <boots color="7F3FB2">leather boots</boots>
    </kit>
    <kit name="cyan" parents="spawn">
      <helmet color="4C7F99">leather helmet</helmet>
      <chestplate>gold chestplate</chestplate>
      <leggings>chainmail leggings</leggings>
      <boots color="4C7F99">leather boots</boots>
    </kit>
  </kits>
  <toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
  </toolrepair>
  <itemremove>
    <item>arrow</item>
  </itemremove>
  <spawns>
    <spawn team="Purple Team" yaw="90" kit="purple">
      <cuboid min="73,17,-1" max="72,17,-1" />
    </spawn>
    <spawn team="Cyan Team" yaw="-90" kit="cyan">
      <cuboid min="-71,17,-1" max="-70,17,-1" />
    </spawn>
    <default yaw="180">
      <cuboid min="0,50,0" max="2,50,-2" />
    </default>
  </spawns>
  <killreward>
    <item amount="1">golden apple</item>
  </killreward>
  <king>
    <hills>
      <hill name="Hill" capture-time="15s" point="1" show-progress="true">
        <capture><cuboid min="-7,12,6" max="9,12,-9" /></capture>
        <progress><cuboid min="-7,12,6" max="9,12,-9" /></progress>
        <captured><cuboid min="0,0,0" max="0,0,0"/></captured>
      </hill>
    </hills>
  </king>
  <score>
    <limit>400</limit>
    <king/>
  </score>
  <regions>
    <cuboid name="purple-spawn" min="66,16,-8" max="74,20,6" />
    <cuboid name="cyan-spawn" min="-64,16,7" max="-72,20,-8" />
    <cylinder name="main-area" center="1,0,-1" radius="75" height="30" />
  </regions>
  <!-- spawn protection -->
  <apply enter="only-purple" message="Do not try to enter, this is protected.">
    <region name="purple-spawn" />
  </apply>
  <apply enter="only-cyan" message="Do not try to enter, this is protected.">
    <region name="cyan-spawn" />
  </apply>
  <!-- end of spawn protection -->
  <!-- non modifiable -->
  <apply block="deny-all">
    <region name="main-area" />
  </apply>
</map>
