<?xml version="1.0"?> 
<map proto="1.3.3"> 
<name>Tired</name> 
<version>1.2.0</version> 
<objective>Be the team with the most kills after 6.40 minutes.</objective> 
<authors> 
    <author contribution="Block Design/Layout/Map Design">Guicotsch</author> 
    <author contribution="XML/Map Design">DoomRobotBoy</author> 
</authors> 
<contributors> 
    <contributor contribution="MapDev Server Host">jollee999</contributor> 
</contributors> 
<rules> 
    <rule>Fall and suffocation damage are disabled!</rule> 
</rules> 
<teams> 
    <team name="red" color="red" max="16" max-overfill="18">Red Team</team> 
    <team name="blue" color="blue" max="16" max-overfill="18">Blue Team</team> 
</teams> 
<spawns> 
    <default kit="all" yaw="90"> 
        <cylinder base="9.5,10,-55.5" radius="1" height="0"/>  
    </default> 
    <spawn team="red" kit="red" yaw="0"> 
        <cuboid min="-11,34,-123" max="-12,34,-125" />
        <cuboid min="24,34,-125" max="25,34,-123" />
    </spawn> 
    <spawn team="blue" kit="blue" yaw="-180"> 
        <cuboid min="-7,34,13" max="-8,34,11" />
        <cuboid min="29,34,13" max="28,34,11" />
    </spawn> 
</spawns> 
<kits> 
    <kit name="all" potion-particles="false"> 
        <potion duration="oo">night vision</potion> 
        <potion duration="oo" amplifier="1">speed</potion> 
        <potion duration="oo" amplifier="3">jump</potion> 
    </kit> 
    <kit name="teams" parents="all" potion-particles="false"> 
        <item slot="0" damage="-3000" enchantment="damage all:10">iron sword</item> 
        <item slot="1" damage="-3000" enchantment="arrow damage:10">bow</item>
        <item slot="2" amount="2">arrow</item>
        <item slot="3" amount="64">vine</item> 
        <potion duration="3" amplifier="100">damage resistance</potion> 
        <potion duration="3" amplifier="100">heal</potion> 
    </kit> 
    <kit name="red" parents="teams"> 
        <helmet damage="-3000" color="993333">leather helmet</helmet> 
        <chestplate damage="-3000" locked="false" color="993333">leather chestplate</chestplate> 
    </kit> 
    <kit name="blue" parents="teams"> 
        <helmet damage="-3000" color="334CB2">leather helmet</helmet> 
        <chestplate damage="-3000" locked="false" color="334CB2">leather chestplate</chestplate> 
    </kit> 
</kits> 
<filters> 
    <filter name="only-vine"> 
        <block>vine</block> 
    </filter> 
</filters> 
<regions> 
    <apply block="only-vine"> 
        <rectangle min="-oo,-oo" max="oo,oo"/> 
    </apply> 
</regions> 
<score> 
    <time>6m40s</time> 
</score> 
<rage/> 
<itemremove> 
    <item>iron sword</item> 
    <item>bow</item> 
    <item>arrow</item> 
    <item>potion</item> 
    <item>glass bottle</item> 
    <item>leather helmet</item> 
    <item>leather chestplate</item> 
    <item>vine</item>
    <item>gold hoe</item>
</itemremove> 
<disabledamage> 
    <damage>fall</damage> 
    <damage>suffocation</damage> 
</disabledamage>
<killreward> 
    <filter> 
        <kill-streak count="3"/> 
    </filter> 
    <item name="`bSpeed Upgrade II" damage="8226" potions="speed:6000:1:true">potion</item> 
</killreward> 
<hunger> 
    <depletion>off</depletion> 
</hunger> 
<timelock>on</timelock> 
</map>
